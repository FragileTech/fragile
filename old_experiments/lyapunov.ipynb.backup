    "import torch\n",
    "import numpy as np\n",
    "import holoviews as hv\n",
    "from holoviews import opts\n",
    "import panel as pn\n",
    "\n",
    "# Enable Bokeh backend\n",
    "hv.extension('bokeh')\n",
    "pn.extension()\n",
    "\n",
    "from fragile.euclidean_gas import (\n",
    "    EuclideanGas,\n",
    "    EuclideanGasParams,\n",
    "    LangevinParams,\n",
    "    CloningParams,\n",
    "    SwarmState,\n",
    "    VectorizedOps,\n",
    ")\n",
    "from fragile.bounds import TorchBounds\n",
    "from fragile.lyapunov import (\n",
    "    identify_high_error_clusters,\n",
    ")\n",
    "\n",
    "print(\"âœ“ Imports loaded successfully\")"
